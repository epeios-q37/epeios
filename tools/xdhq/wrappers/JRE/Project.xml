			<Modules Group="SCL">
				<xpp:expand select="SCL"/>
			</Modules>
		</Modules>
		<Modules Group="Shared" Path="../">
			<Module Path="../">prtcl</Module>
			<Module>proxy</Module>
			<Module>prxy_cmn</Module>
			<Module>prxy_recv</Module>
			<Module>prxy_send</Module>
			<Module>tree</Module>
