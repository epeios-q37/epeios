<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet	version="1.0"                xmlns:xsl="http://www.w3.org/1999/XSL/Transform"                xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">	<xsl:output method="xml"              indent="yes"              encoding="utf-8"              omit-xml-declaration="yes"              standalone="yes"/>	<xsl:template match="/">		<xsl:apply-templates select="PredefinedBackends"/>	</xsl:template>	<xsl:template match="PredefinedBackends">		<xsl:element name="menupopup">			<xsl:apply-templates select="PredefinedBackend"/>		</xsl:element>	</xsl:template>	<xsl:template match="PredefinedBackend">		<xsl:element name="menuitem">			<xsl:attribute name="value">				<xsl:value-of select="@id"/>			</xsl:attribute>			<xsl:attribute name="label">				<xsl:value-of select="@Alias"/>			</xsl:attribute>			<xsl:if test="@id=../@Default">				<xsl:attribute name="xex:selected"                       namespace="http://zeusw.org/epeios/xex">true</xsl:attribute>			</xsl:if>		</xsl:element>	</xsl:template></xsl:stylesheet>