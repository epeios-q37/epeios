<?xml version="1.0" encoding="ISO-8859-1"?><xsl:stylesheet	version="1.0"                xmlns:xsl="http://www.w3.org/1999/XSL/Transform"                xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">	<xsl:output method="xml"              indent="yes"              encoding="ISO-8859-1"              omit-xml-declaration="yes"              standalone="yes"/>	<xsl:template match="/">		<xsl:apply-templates select="PredefinedProjects"/>	</xsl:template>	<xsl:template match="PredefinedProjects">		<xsl:element name="menupopup">			<xsl:apply-templates select="PredefinedProject"/>		</xsl:element>	</xsl:template>	<xsl:template match="PredefinedProject">		<xsl:element name="menuitem">			<xsl:attribute name="value">				<xsl:value-of select="@id"/>			</xsl:attribute>			<xsl:attribute name="label">				<xsl:value-of select="@Alias"/>			</xsl:attribute>		</xsl:element>	</xsl:template></xsl:stylesheet>